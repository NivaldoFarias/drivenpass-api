{
  "name": "drivenpass-api",
  "version": "1.0.0",
  "description": "DrivenPass API",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "ts:start": "ts-node src/server.ts",
    "dev": "tsnd --respawn --transpile-only src/server.ts",
    "debug": "tsnd --respawn --transpile-only --debug src/server.ts",
    "migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": {
    "name": "Nivaldo Farias",
    "url": "https://github.com/NivaldoFarias"
  },
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cryptr": "^6.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.3",
    "@types/pg": "^8.6.5",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
